    "description": "This Bundle extends friendsofsymfony/rest-bundle to easy develop rest applications",
    "keywords": ["rest"],
    "license": "MIT",
    "authors": [
        {
            "name": "Stanislav Pivovartsev",
            "email": "staspivovartsev@gmail.com"
        }
    ],

    "require": {
        "php": ">=7",
        "friendsofsymfony/rest-bundle": "~2.0",
        "doctrine/orm": "^2.5",
        "symfony/form": "~2.3|~3.0",
        "jms-serializer/serializer-bundle": "1.1.4",
        "liip/functional-test-bundle": "~1.6",
        "doctrine/doctrine-fixtures-bundle": "~2.3",
        "phpunit/phpunit": "^5.4"
    },

    "require-dev": {
        "staspiv/review-bundle": "~1.0"
    },

    "autoload":{
        "psr-4":{
            "StasPiv\\RestBundle\\": "src"
        }
    }
